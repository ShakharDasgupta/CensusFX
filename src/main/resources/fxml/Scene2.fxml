<GridPane hgap="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" styleClass="root" vgap="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Text id="title-text" strokeType="OUTSIDE" strokeWidth="0.0" text="CensusFX" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
        <SplitPane dividerPositions="0.5" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.rowSpan="4">
            <items>
                <VBox alignment="CENTER" spacing="1.0">
                    <children>
                        <Text fx:id="place1Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="place-text" textAlignment="CENTER" wrappingWidth="250.0" />
                        <Text fx:id="county1Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="county-text" textAlignment="CENTER" wrappingWidth="250.0" />
                        <Text fx:id="state1Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="state-text" textAlignment="CENTER" wrappingWidth="250.0" />
                        <PieChart fx:id="place1Chart" />
                    </children>
                </VBox>
                <VBox alignment="CENTER" spacing="1.0">
                    <children>
                        <Text fx:id="place2Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="place-text" textAlignment="CENTER" wrappingWidth="250.0" />
                        <Text fx:id="county2Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="county-text" textAlignment="CENTER" wrappingWidth="250.0" />
                        <Text fx:id="state2Text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="state-text" textAlignment="CENTER" wrappingWidth="250.0" />
